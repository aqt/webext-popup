{
	"manifest_version": 2,
	"author": "aqt",

	"homepage_url": "https://github.com/aqt/webext-popup",
	"name": "PopUp",
	"version": "1.1",
	"description": "Create a popup of the current page with minimal browser ui",

	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png"
	},

	"permissions": [ "activeTab" ],

	"browser_action": {
		"default_icon": {
			"16": "icons/icon16.png",
			"32": "icons/icon32.png",
			"48": "icons/icon48.png"
		},

		"default_title": "PopUp",
		"default_popup": "popup.html"
	}
}
